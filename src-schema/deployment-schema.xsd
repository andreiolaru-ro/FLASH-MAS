<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://flash.xqhs.net/deployment-schema"
	xmlns:tns="http://flash.xqhs.net/deployment-schema" elementFormDefault="qualified">
	<element name="scenario">
		<complexType>
			<sequence>
				<element name="config" maxOccurs="1" minOccurs="0">
					<complexType>
						<sequence>
							<element name="parameter" minOccurs="0" maxOccurs="unbounded">
								<complexType>
									<attribute name="name" type="string" use="required" />
									<attribute name="value" type="string" use="required" />
								</complexType>
							</element>
						</sequence>
						<attribute name="center" type="string" use="optional" />
						<attribute name="here" type="string" use="optional" />
					</complexType>
				</element>
				<element name="support" maxOccurs="unbounded" minOccurs="0">
					<complexType>
						<sequence>
							<element name="parameter" minOccurs="0" maxOccurs="unbounded">
								<complexType>
									<attribute name="name" type="string" use="required" />
									<attribute name="value" type="string" use="required" />
								</complexType>
							</element>
						</sequence>
						<attribute name="kind" type="string" use="required" />
						<attribute name="id" type="string" use="optional" />
					</complexType>
				</element>
				<element name="loader" maxOccurs="unbounded" minOccurs="0">
					<complexType>
						<sequence>
							<element name="parameter" minOccurs="0" maxOccurs="unbounded">
								<complexType>
									<attribute name="name" type="string" use="required" />
									<attribute name="value" type="string" use="required" />
								</complexType>
							</element>
						</sequence>
						<attribute name="for" type="string" use="required" />
						<attribute name="classpath" type="string" use="required" />
					</complexType>
				</element>
				<element name="package" type="string" minOccurs="0"
					maxOccurs="unbounded" />
				<element name="agent" maxOccurs="unbounded" minOccurs="0">
					<complexType>
						<sequence>
							<element name="component" minOccurs="0" maxOccurs="unbounded">
								<complexType>
									<sequence>
										<element name="parameter" minOccurs="0" maxOccurs="unbounded">
											<complexType>
												<attribute name="name" type="string" use="required" />
												<attribute name="value" type="string" use="required" />
											</complexType>
										</element>
									</sequence>
									<attribute name="name" type="string" use="required" />
									<attribute name="classpath" type="string" use="optional" />
								</complexType>
							</element>
							<element name="parameter" minOccurs="0" maxOccurs="unbounded">
								<complexType>
									<attribute name="name" type="string" use="required" />
									<attribute name="value" type="string" use="required" />
								</complexType>
							</element>
						</sequence>
						<attribute name="name" type="string" use="optional" />
					</complexType>
				</element>
				<element name="timeline" maxOccurs="1" minOccurs="0">
					<complexType>
						<sequence>
							<element name="event" minOccurs="1" maxOccurs="unbounded">
								<complexType>
									<sequence>
										<element name="message" maxOccurs="unbounded"
											minOccurs="0">
											<complexType>
												<sequence>
													<element name="to" type="string" minOccurs="1"
														maxOccurs="unbounded" />
													<element name="content" type="string" minOccurs="0"
														maxOccurs="1" />
												</sequence>
											</complexType>
										</element>
									</sequence>
									<attribute name="time" type="positiveInteger" use="required" />
								</complexType>
							</element>
						</sequence>
						<attribute name="duration" type="positiveInteger" use="optional">
						</attribute>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
</schema>
